{
  "name": "framework-compass-chart-editor",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com:e-xtrategy/framework-compass-chart-editor.git"
  },
  "scripts": {
    "predeploy": "npm run build",
    "build": "webpack --config ./webpack.config.prod.js",
    "start": "webpack-dev-server --config ./webpack.config.dev.js",
    "test": "jest",
    "deploy": "gh-pages -d dist"
  },
  "license": "MIT",
  "dependencies": {
    "@webcomponents/custom-elements": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-custom-element-classes": "0.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "css-loader": "0.28.11",
    "file-loader": "0.11.2",
    "gh-pages": "1.0.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.0.0-rc.1",
    "jest": "22.4.3",
    "lint-staged": "7.0.4",
    "prettier": "1.12.0",
    "standard": "10.0.3",
    "style-loader": "0.21.0",
    "url-loader": "0.5.9",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "3.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "HTMLElement",
      "describe",
      "test",
      "expect"
    ]
  }
}
